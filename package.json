{
  "name": "ty-step",
  "version": "1.0.1",
  "description": "简单的异步流程控制方法，可以并行和串行执行给出的方法序列，适用于nodejs和浏览器环境",
  "main": "index.min.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha -t 50000&&node ./node_modules/karma/bin/karma start",
    "lint": "node ./node_modules/eslint/bin/eslint -f table -o eslintreport --fix index.js",
    "build": "node ./node_modules/babel-cli/bin/babel index.js --out-file index.min.js --no-comments --minified"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lovetingyuan/ty-step.git"
  },
  "keywords": [
    "control-flow",
    "async",
    "sync",
    "step",
    "parallel",
    "serial",
    "executor"
  ],
  "author": "tingyuan <1932294867@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lovetingyuan/ty-step/issues"
  },
  "homepage": "https://github.com/lovetingyuan/ty-step#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-latest": "^6.22.0",
    "browserify": "^14.0.0",
    "chai": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "karma": "^1.4.1",
    "karma-browserify": "^5.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "mocha": "^3.2.0"
  }
}
